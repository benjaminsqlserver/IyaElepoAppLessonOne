{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Product Supplier Name",
                              "component": "ProductSupplierName",
                              "name": "ProductSupplierNameLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 300,
                              "type": "textbox",
                              "name": "ProductSupplierName",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${productsupplier.ProductSupplierName}"
                            },
                            {
                              "type": "required-validator",
                              "name": "ProductSupplierNameRequiredValidator",
                              "component": "ProductSupplierName",
                              "text": "ProductSupplierName is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Office Address",
                              "component": "OfficeAddress",
                              "name": "OfficeAddressLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "type": "textbox",
                              "name": "OfficeAddress",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${productsupplier.OfficeAddress}"
                            },
                            {
                              "type": "required-validator",
                              "name": "OfficeAddressRequiredValidator",
                              "component": "OfficeAddress",
                              "text": "OfficeAddress is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Email",
                              "component": "Email",
                              "name": "EmailLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 50,
                              "type": "textbox",
                              "name": "Email",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${productsupplier.Email}"
                            },
                            {
                              "type": "required-validator",
                              "name": "EmailRequiredValidator",
                              "component": "Email",
                              "text": "Email is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Phone Number",
                              "component": "PhoneNumber",
                              "name": "PhoneNumberLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 50,
                              "type": "textbox",
                              "name": "PhoneNumber",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${productsupplier.PhoneNumber}"
                            },
                            {
                              "type": "required-validator",
                              "name": "PhoneNumberRequiredValidator",
                              "component": "PhoneNumber",
                              "text": "PhoneNumber is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Contact Person",
                              "component": "ContactPerson",
                              "name": "ContactPersonLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 150,
                              "type": "textbox",
                              "name": "ContactPerson",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${productsupplier.ContactPerson}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update ProductSupplier",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "ConData.updateProductSupplier",
                        "parameters": [
                          {
                            "name": "ProductSupplierID",
                            "value": "${parameters.ProductSupplierID}"
                          },
                          {
                            "name": "ProductSupplier",
                            "value": "${productsupplier}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${productsupplier}",
                            "condition": ""
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "IyaElepoApp.Models.ConData.ProductSupplier",
                  "data": "${productsupplier}",
                  "visible": "${productsupplier != null}"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "ConData.getProductSupplierByProductSupplierID",
        "parameters": [
          {
            "name": "ProductSupplierID",
            "value": "${parameters.ProductSupplierID}"
          }
        ],
        "then": [
          {
            "name": "productsupplier",
            "type": "set",
            "value": "${result}",
            "valueType": "IyaElepoApp.Models.ConData.ProductSupplier"
          }
        ],
        "error": [],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Product Supplier",
  "layout": "Main"
}